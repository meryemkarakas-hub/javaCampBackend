package oopIntro;

public class Product {
	int id;
	 String name;
	 double unitPrice;
	 String detail;
	 public Product() 
	 {
	 }
	public Product(int id, String name, double unitPrice, String detail) {
		super();
		this.id = id;
		this.name = name;
		this.unitPrice = unitPrice;
		this.detail = detail;
	}
	 
	/*public Product() {//constructor
		System.out.println("Ben calistim");
